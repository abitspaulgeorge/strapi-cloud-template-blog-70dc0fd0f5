        "sections.preheading",
        "sections.title-text-two-columns",
        "sections.logos-carousel",
        "sections.featured-numbers-slider",
        "sections.professional-service",
        "sections.managed-service-slider",
        "sections.title-text-with-button",
        "sections.title-with-parallax",
        "sections.img-text-with-dark-background",
        "sections.intro-title-button-text",
        "sections.icon-grid",
        "sections.intro-text-one-column",
        "sections.intro-text-pattern",
        "sections.intro-text-two-columns",
        "sections.intro-title-text",
        "sections.image-text-counters",
        "sections.image-video-full",
        "sections.featured-number-grid",
        "sections.featured-icon-grid",
        "sections.image-title-list-dark",
        "sections.parallax-text-two-columns",
        "sections.text-counters",
        "sections.icon-text-slider",
        "sections.title-block",
        "sections.title-text-columns-pattern",
        "sections.image-title-text-pattern",
        "sections.story-slider",
        "sections.two-images-counters"
      ]
